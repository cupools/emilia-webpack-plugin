{
  "name": "emilia-webpack-plugin",
  "version": "0.0.1",
  "description": "webpack plugin to handle sprite pictures, base on Emilia",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha --dir test/coverage --root src --include-all-sources true -- --compilers js:babel-core/register -i test/*.test.js",
    "coverage": "cat test/coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "dev": "rm -rf lib && node node_modules/babel-cli/bin/babel src -d lib -D -w"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "chai-css": "0.0.4",
    "coveralls": "^2.11.12",
    "cross-env": "^2.0.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^2.3.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-merge": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cupools/emilia-webpack-plugin"
  },
  "keywords": [
    "css",
    "sprite",
    "stylesheet",
    "webpack"
  ],
  "author": "liyh <cupools@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cupools/emilia-webpack-plugin/issues"
  },
  "homepage": "https://github.com/cupools/emilia-webpack-plugin#readme",
  "dependencies": {
    "file-loader": "^0.9.0",
    "sprite-tool": "github:cupools/sprite-tool"
  }
}
